{
  "azure": {
    "storage": {
      "accessKey": "your-azure-storage-access-key",
      "account": "your-azure-storage-account-name"
    },
    "queue": {
      "webhooks": "webhooks"
    }
  },
  "bodyParser": {
    "limit": "5mb"
  },
  "endpoints": {
    "whitelisted": [
      "/",
      "/ping"
    ]
  },
  "env": "development",
  "github": {
    "appId": "",
    "privateKeyPath": "key.pem",
    "secret": ""
  },
  "platform": {
    "repo": "platform-release"
  },
  "port": 3000,
  "rollbar": {
    "env": "development",
    "token": "your-server-project-token"
  },
  "setup": {
    "interval": 3000,
    "visibilityTimeout": 30,
    "numOfMessages": 1
  },
  "tokens": {}
}